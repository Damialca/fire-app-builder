{
  "cooker": "DynamicParser",
  "format": "json",
  "model": "com.amazon.android.model.content.Content",
  "modelType": "array",
  "translator": "ContentTranslator",
  "query": "$.playlist",
  "matchList": [
    "title@title",
    "mediaid@id",
    "description@description",
    "sources]file@url",
    "image@cardImageUrl",
    "image@backgroundImageUrl"
  ]
}